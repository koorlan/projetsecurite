clear
echo "INITIALISATION"
echo 
echo "Appuyez sur une touche pour voir les instructions"
echo "(./secu -h)"
read -n 1 -s
clear
./secu -h

./ java -jar Init.jar
for (( c=1; c<=5; c++ )) //c= nb machines du réseau social 
do
   echo "Creating the $c machine"
   sudo xterm -title "User$c" -e interface BD // il faut les droit su pour changer le nom d'une fenêtre ...
done

